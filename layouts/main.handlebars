<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{{htmlTitle}}</title>
    <link rel="stylesheet" href="{{projectAssets}}/vendor/adcssy/adcssy.min.css">
    <link rel="stylesheet" href="{{projectAssets}}/vendor/highlight/styles/default.css">
    <link rel="stylesheet" href="{{projectAssets}}/css/main.css" id="site_styles">
    <script src="{{yuiSeedUrl}}"></script>
  </head>
  <body>
    <div class="page">
      <div class="page-viewport">
        <nav class="nav-side">
          <header class="nav-side-header">
            <button
              class="nav-toggle nav-side-toggle"
              onclick="this.parentNode.parentNode.classList.toggle('nav-side--expand')">
              <span class="nav-toggle-icon"></span>
            </button>
            <a href="#">
              <div class="nav-side-header-logo" style="{{#if projectLogo}}background-image:url({{projectLogo}}){{/if}}"></div>
              <div class="nav-side-header-title">{{projectName}} {{projectVersion}}</div>
            </a>
          </header>
          <section class="nav-side-body">
            {{>sidebar}}
          </section>
        </nav>
        <main class="page-main page-main--scroll">
        <section class="page-content">
{{>layout_content}}
        </section>
        </main>
      </div>
    </div>

    <script src="{{projectAssets}}/vendor/highlight/highlight.pack.min.js"></script>
    <script src="{{projectAssets}}/vendor/jquery/jquery.min.js"></script>
    <script src="{{projectAssets}}/js/tabs.js"></script>
    <script src="{{projectAssets}}/js/options.js"></script>
    <script src="{{projectAssets}}/js/search.js"></script>
    <script>$('pre code').addClass('js')</script>
    <script>hljs.initHighlightingOnLoad()</script>
  </body>
</html>
