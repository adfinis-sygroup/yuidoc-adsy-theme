<div class="method {{#if access}}{{access}}{{/if}} {{#if extended_from}}inherited{{/if}} {{#if deprecated}}deprecated{{/if}}" id="method_{{name}}">
  <h4 class="title">{{name}}</h4>

  <em class="params">({{#params}}<span class="param">{{name}}</span>{{/params}})</em>

  {{#if deprecated}}<span class="flag" title="{{deprecationMessage}}">deprecated</span>{{/if}}
  {{#if access}}<span class="flag">{{access}}</span>{{/if}}
  {{#if final}}<span class="flag">final</span>{{/if}}
  {{#if static}}<span class="flag">static</span>{{/if}}
  {{#if chainable}}<span class="flag">chainable</span>{{/if}}
  {{#if async}}<span class="flag">async</span>{{/if}}

  <div class="meta">
    <em>
      {{#if overwritten_from}}
        Inherited from
        <a href="{{crossLinkRaw overwritten_from/class}}#property_{{overwritten_from/name}}">
          {{overwritten_from/class}}
        </a>
        {{#if foundAt}}
          but overwritten in
        {{/if}}
      {{else}}
        {{#if extended_from}}
          Inherited from
          <a href="{{crossLinkRaw extended_from}}#property_{{name}}">{{extended_from}}</a>:
        {{else}}
          {{#providedBy}}
            Provided by the <a href="../modules/{{.}}.html">{{.}}</a> module.
          {{/providedBy}}
          {{#if foundAt}}
            Defined in
          {{/if}}
        {{/if}}
      {{/if}}
      {{#if foundAt}}
        <a href="{{foundAt}}">{{{file}}}:{{{line}}}</a>
      {{/if}}
    </em>
  </div>

  <p>{{{methodDescription}}}</p>

  {{#example}}
    <h4>Example:</h4>
{{{.}}}
  {{/example}}

  {{#if params}}
    <h4>Parameters:</h4>

    <ul class="pad-left param-list">
      {{#params}}
        <li>
          <strong>{{name}}</strong>
          <span class="type">{{#crossLink type}}{{/crossLink}}</span>
          {{#if optional}}<span>(optional)</span>{{/if}}
          <div class="pad-left">{{{description}}}</div>
        </li>
      {{/params}}
    </ul>
  {{/if}}

  {{#return}}
    <h4>Returns:</h4>

    <div class="pad-left">
      {{#if type}}
        <span class="type">{{#crossLink type}}{{/crossLink}}</span>
        <div class="pad-left">{{{description}}}</div>
      {{else}}
        {{{description}}}
      {{/if}}
    </div>
  {{/return}}

  {{#throws}}
    <h4>Throws:</h4>
    <div class="pad-left">
      {{#if type}}
        <span class="type">{{#crossLink type}}{{/crossLink}}</span>
        <div class="pad-left">{{{description}}}</div>
      {{else}}
        {{{description}}}
      {{/if}}
    </div>
  {{/throws}}
</div>
